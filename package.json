{
  "name": "setup-rust-lambda",
  "version": "1.0.0",
  "description": "Setup Rust Lambda toolchain for GitHub Actions",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/<your-username>/setup-rust-lambda.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "ncc build wrapper/cargo.js --out wrapper/dist && ncc build index.js --out dist",
    "lint": "semistandard --fix"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.1",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "semistandard": "^17.0.0"
  },
  "semistandard": {
    "env": ["jest"],
    "ignore": [
      "**/dist/**"
    ]
  }
}
